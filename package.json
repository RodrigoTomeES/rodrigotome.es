{
  "name": "rodrigotome.es",
  "packageManager": "yarn@4.0.1",
  "type": "module",
  "version": "1.0.0",
  "license": "GPL-3.0+",
  "author": "Rodrigo Tom√© <hi@rodrigotome.es>",
  "scripts": {
    "dev": "astro dev",
    "start": "astro preview",
    "build": "astro build",
    "preview": "yarn build && astro preview",
    "astro": "astro",
    "prepare": "husky install",
    "lint": "eslint --fix \"**/*.{ts,tsx,js,cjs,mjs,jsx,astro}\"",
    "lint:styles": "stylelint --fix \"**/*.css\"",
    "prettier": "prettier . --write",
    "commit": "cz",
    "update": "yarn set version stable",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "7.6.4",
    "@astrojs/sitemap": "3.0.3",
    "@astrojs/tailwind": "5.0.2",
    "@commitlint/cli": "18.4.0",
    "@commitlint/config-conventional": "18.4.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@jasikpark/astro-svg-loader": "0.2.0",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "astro": "3.5.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-astro": "0.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.1",
    "prettier-plugin-tailwindcss": "0.5.7",
    "prettier-stylelint": "0.4.2",
    "sharp": "0.32.6",
    "stylelint": "15.11.0",
    "stylelint-config-idiomatic-order": "9.0.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-tailwindcss": "0.0.7",
    "tailwind-scrollbar-hide": "1.1.7",
    "tailwindcss": "3.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  }
}
